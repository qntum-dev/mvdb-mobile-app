{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "alwaysStrict": true,
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "isolatedModules": true,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "@cloudflare/workers-types"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}